@using FieldTool.Models;
<div class="row clone">
    <input class="witnessID" hidden value="@Model.WitnessId">
    <div class="form-group">
        <label>Witness Name *</label>
        <input type="text" class="form-control required" value="@Model.WitnessName">
    </div>
    <div class="form-group">
        <label >Witness Address *</label>
        <input type="text" class="form-control required" value="@Model.WitnessAddress">
    </div>
    <div class="row addedSection">
        <div class="form-group col-md-9 delBtnAlign">
            <label>Witness Phone *</label>
            <input type="tel" class="form-control required" value="@Model.WitnessPhone">
        </div>
        <div class="col-md-3">
            <button type="button" class="btn btn-danger btnRemoveWitness">Remove</button>
        </div>
    </div>
</div>
